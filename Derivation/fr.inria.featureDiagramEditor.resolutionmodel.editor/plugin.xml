<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="resolutionmodel.presentation.ResolutionmodelModelWizardID"
            name="%_UI_ResolutionmodelModelWizard_label"
            class="resolutionmodel.presentation.ResolutionmodelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ResolutionmodelModelFile.gif">
         <description>%_UI_ResolutionmodelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="resolutionmodel.presentation.ResolutionmodelEditorID"
            name="%_UI_ResolutionmodelEditor_label"
            icon="icons/full/obj16/ResolutionmodelModelFile.gif"
            extensions="resolutionmodel"
            class="resolutionmodel.presentation.ResolutionmodelEditor"
            contributorClass="resolutionmodel.presentation.ResolutionmodelActionBarContributor">
      </editor>
   </extension>

</plugin>
